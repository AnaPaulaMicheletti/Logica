programa {
  funcao inicio() {
    inteiro ano
    escreva("Qual ano vc quer subtrair: ")
    leia(ano)

    inteiro idade
    escreva("Qual a sua idade? ")
    leia(idade)
    
    inteiro subtrair (ano - idade)
    escreva("Subtrair ", ano, " ao ", idade, " resulta em ", subtrair)

    inteiro resulta
    escreva(ano - idade) 
  }
}
